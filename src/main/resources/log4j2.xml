<?xml version="1.0" encoding="UTF-8"?>
<Configuration packages="com.uci.orchestrator" status="info" name="orchestrator-log">
	<Appenders>
		<Console name="info" target="SYSTEM_OUT">
			<PatternLayout
				pattern="%d{HH:mm:ss.SSS} info %highlight(%-5p) [%-7t] %F:%L - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="INFO">
			<AppenderRef ref="info" />
		</Root>
	</Loggers>
</Configuration>
